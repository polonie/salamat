extends layout.pug
block css
	link(rel="stylesheet" href="./css/rent.css")
block content
	.rent-page-content
		.rent-page-container
			.trade-area-imgs
				.trade-area-header
					h2.trade-area-title Торговые площади
				.trade-area-content
					.trade-area-items.clearfix
						each item in rent_imgs
							.trade-area-item
								- var url = '/img/rent/' + item
								img.trade-area-img-item(src= url)
			.for-rent-block
				.for-rent-header
					h2.for-rent-title Сдаются в аренду помешения
				.for-rent-content
					.for-rent-list
						ul.rent-list
							li.rent-list-item Саламат 2
								span.rent-list-item-span торг. площадь 122 кв.м
							li.rent-list-item Саламат 3
								span.rent-list-item-span торг. площадь 90 кв.м
							li.rent-list-item Саламат 5
								span.rent-list-item-span торг. площадь 100 кв.м
							li.rent-list-item Прилавки
								span.rent-list-item-span 2 по 5 кв.м
							li.rent-list-item Склад
								span.rent-list-item-span 24 кв.м
							li.rent-list-item Склад
								span.rent-list-item-span 9 кв.м

